{
  "name": "scrapper-poc-dynamic",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist-server/bin/index.js",
    "start-dev": "nodemon --delay 1 dist-server/bin/index.js",
    "start-itellij-debug": "node $NODE_DEBUG_OPTION dist-server/bin/index.js",
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rimraf dist-server/**",
    "tsc": "tsc -p tsconfig.json",
    "tscw": "tsc --watch -p tsconfig.json",
    "test": "mocha --reporter min --inline-diffs dist-server/**/*.spec.js",
    "test-watch": "npm run test -- -w"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "chrome-remote-interface": "^0.22.0",
    "cookie-parser": "~1.4.3",
    "dotenv": "^4.0.0",
    "express": "~4.14.1",
    "fs-extra": "^3.0.1",
    "inversify": "^3.3.0",
    "lighthouse": "^2.0.0",
    "lodash": "^4.17.4",
    "node-cleanup": "^2.1.2",
    "node-horseman": "^3.2.0",
    "reflect-metadata": "^0.1.10",
    "timeout-as-promise": "^1.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.1",
    "@types/chai": "^3.4.35",
    "@types/chai-as-promised": "0.0.30",
    "@types/cookie-parser": "^1.3.30",
    "@types/dotenv": "^4.0.0",
    "@types/express": "^4.0.35",
    "@types/fs-extra": "^3.0.2",
    "@types/lodash": "^4.14.64",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.11",
    "@types/sinon": "^1.16.36",
    "@types/sinon-chai": "^2.7.27",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "typescript": "^2.2.1"
  }
}
